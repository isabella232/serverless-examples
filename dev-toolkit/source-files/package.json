    "dev-toolkit": "^7.3.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "prettier": "^1.14.3"
  }
}
